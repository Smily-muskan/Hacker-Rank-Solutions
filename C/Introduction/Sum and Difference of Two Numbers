#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main()
{
	  int n1,n2,a,b;
    float m1,m2,c,d;
    scanf("%d%d",&n1,&n2);
    a=n1+n2;
    b=n1-n2;
    printf("%d %d \n",a,b);
    scanf("%f%f",&m1,&m2);
    c=m1+m2;
    d=m1-m2;
    printf("%.1f %.1f",c,d);
    return 0;
}
